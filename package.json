    "@types/webpack-env": "^1.13.9",
    "awesome-typescript-loader": "^5.2.1",
    "compression-webpack-plugin": "^2.0.0",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.6.0",
